{
  "name": "admin-api",
  "version": "1.0.0",
  "description": "Admin API build using Express.js",
  "license": "ISC",
  "author": "N-eeraj",
  "type": "module",
  "main": "app.ts",
  "scripts": {
    "dev": "node --watch --env-file=.env --import tsx src/app.ts",
    "command": "node --env-file=.env --import tsx src/commands/index.ts"
  },
  "imports": {
    "#middlewares/*": "./src/middlewares/*.ts",
    "#routes/*": "./src/routes/*.ts",
    "#controllers/*": "./src/controllers/*.ts",
    "#services/*": "./src/services/*.ts",
    "#commands/*": "./src/commands/*.ts",
    "#utils/*": "./src/utils/*.ts",
    "#docs/*": "./src/modules/docs/*",
    "#catalogue/*": "./src/modules/admin/catalogue/*",
    "#src/*": "./src/*",
    "#jsonDocs/*": "./docs/*.json",
    "#app/*": "./*"
  },
  "dependencies": {
    "@types/pino-http": "^5.8.4",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/multer": "^2.0.0",
    "@types/swagger-ui-express": "^4.1.8"
  }
}
