 {
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Unique subcategory ID.",
      "example": "01a0a0123a01a012a0a012a0"
    },
    "category": {
      "type": "string",
      "description": "Category ID of the subcategory.",
      "example": "01a0a0123a01a012a0a012a0"
    },
    "name": {
      "type": "string",
      "description": "Name of the subcategory.",
      "example": "Smart Phone"
    },
    "slug": {
      "type": "string",
      "description": "Unique and short name (slug) of the subcategory.",
      "example": "smtphn"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the subcategory was created. Follows ISO 8601 format."
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time",
      "description": "Timestamp when the subcategory was last updated. Follows ISO 8601 format."
    }
  }
}
